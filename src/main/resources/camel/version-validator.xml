<routes xmlns="http://camel.apache.org/schema/spring">
    <route id="version-validation">
        <from uri="direct:version-validator"/>
        <doTry>
            <process ref="VersionValidator"/>
            <doCatch>
                <exception>io.dataspaceconnector.exceptions.VersionNotSupportedException</exception>
                <log message="DOOM! ${body}"/>
            </doCatch>
        </doTry>
    </route>
</routes>
